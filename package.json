{
  "name": "budget-node-serverless",
  "version": "1.0.0",
  "oldstart:ci": "ts-node src/index.ts",
  "oldstart": " npm run prebuild && npm run gcp-build &&  || ts-node src/index.ts ",
  "oldparam": " nodemon --config nodemon.json --host 192.168.1.4 ",
  "mogooseissue": "npm install mongoose typescript @types/mongoose",
  "main": "index.ts",
  "scripts": {
    "gcp-build": "tsc -p .",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc -p .",
    "build-netlify": "cd netlify/functions/rest-server && tsc -p . ",
    "json-move-netlify": "copy netlify\\functions\\rest-server\\userRegisteredJson*.* netlify\\functions\\rest-server\\netlify\\functions\\rest-server\\",
    "remove-index-netlify": "del netlify\\functions\\rest-server\\index*.* ",
    "move-src-netlify": "xcopy src \"netlify/functions/rest-server/\" /isvy && xcopy scratch \"netlify/functions/rest-server/scratch\" /isvy  ",
    "prestart": "npm run build",
    "start": " node index.js",
    "start:log": "env LOGS=stdout nodemon --config nodemon.json | bunyan",
    "start:ci": "ts-node index.ts",
    "deploy": "netlify deploy --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch **/integration/*.(it).ts",
    "test:e2e": "jest --testMatch **/e2e/*.(e2e).ts",
    "test:arch": "jest --testMatch **/arch/*.(spec).ts",
    "test:pact": "jest --testMatch **/contract/*.(pact).ts",
    "sonar": "sonar-scanner.bat"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@netlify/functions": "^2.8.2",
    "@pact-foundation/pact": "^9.16.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.7",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^22.8.2",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "aws-sdk": "^2.1691.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "localstorage-polyfill": "^1.0.1",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "netlify-cli": "^17.37.1",
    "netlify-lambda": "^2.0.16",
    "node-localstorage": "^2.2.1",
    "nodemon": "^2.0.14",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "rand-token": "^0.4.0",
    "rotating-file-stream": "^2.1.6",
    "serverless-http": "^3.2.0",
    "store2": "^2.14.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^8.10.2",
